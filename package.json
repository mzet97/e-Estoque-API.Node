{
  "name": "e-estoque-api.node",
  "version": "1.0.0",
  "description": "Inventory system API with node.js",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist && npx tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/http/server.ts",
    "dev:gateway": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/gateway/GatewayServer.ts",
    "start:gateway": "node dist/gateway/GatewayServer.js",
    "typeorm": "ts-node-dev  -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:run": "pnpm typeorm migration:run -d src/shared/typeorm/index.ts",
    "migration:generate": "pnpm typeorm migration:generate -d src/shared/typeorm/index.ts",
    "migration:revert": "pnpm typeorm migration:revert -d src/shared/typeorm/index.ts",
    "seed:admin": "ts-node-dev  -r tsconfig-paths/register src/shared/typeorm/seeds/admin.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:performance": "jest --testPathPattern=performance",
    "test:load": "node tests/load/load-test.js",
    "test:security": "jest --testPathPattern=security",
    "test:coverage:report": "node tests/utils/CoverageAnalyzer.ts",
    "test:reports": "./scripts/generate-test-reports.sh",
    "test:quality": "npm run lint && npm run test:coverage",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e && npm run test:performance && npm run test:security",
    "test:ci": "npm run lint && npm run test:coverage -- --watchAll=false --ci",
    "db:migrate": "tsx scripts/database/migrate.ts",
    "db:migrate:test": "NODE_ENV=test DATABASE_URL=postgresql://test:test@localhost:5432/test_db npm run db:migrate",
    "db:migrate:rollback": "tsx scripts/database/migrate.ts --action rollback --target",
    "db:migrate:status": "tsx scripts/database/migrate.ts --action status",
    "db:migrate:pending": "tsx scripts/database/migrate.ts --action pending",
    "db:migrate:dry-run": "tsx scripts/database/migrate.ts --dry-run",
    "db:seed": "tsx scripts/database/seed.ts",
    "db:reset": "npm run db:migrate:rollback -- 1.0.0 && npm run db:migrate && npm run db:seed",
    "secrets:rotate": "bash scripts/secrets/rotate-secrets.sh",
    "secrets:rotate:jwt": "bash scripts/secrets/rotate-secrets.sh --type jwt",
    "secrets:rotate:database": "bash scripts/secrets/rotate-secrets.sh --type database",
    "secrets:rotate:api-keys": "bash scripts/secrets/rotate-secrets.sh --type api-keys",
    "secrets:rotate:dry-run": "bash scripts/secrets/rotate-secrets.sh --dry-run",
    "secrets:validate": "node scripts/secrets/validate-secrets.js",
    "secrets:backup": "bash scripts/secrets/backup-secrets.sh",
    "secrets:restore": "bash scripts/secrets/restore-secrets.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mzet97/e-Estoque-API.Node.git"
  },
  "keywords": [
    "node.js",
    "api"
  ],
  "author": "mzet97",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mzet97/e-Estoque-API.Node/issues"
  },
  "homepage": "https://github.com/mzet97/e-Estoque-API.Node#readme",
  "dependencies": {
    "amqplib": "^0.10.3",
    "node-cron": "^3.0.3",
    "uuid": "^11.0.1",
    "prom-client": "^15.1.0",
    "hot-shots": "^10.0.0",
    "boom": "^7.3.0",
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^5.0.1",
    "express-async-handler": "^1.2.0",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "joi": "^17.12.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "multer": "1.4.5-lts.1",
    "pg": "^8.11.3",
    "pino": "^9.5.0",
    "pino-http": "^10.3.0",
    "pino-pretty": "^11.3.0",
    "redis": "^4.6.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tsyringe": "^4.8.0",
    "typeorm": "^0.3.24",
    "uuid": "^11.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.11",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.11.0",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "artillery": "^2.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nock": "^14.0.0",
    "puppeteer": "^24.0.0",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.2"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "engines": {
    "node": ">=22.11.0",
    "pnpm": ">=10.0.0"
  }
}
